{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "functions": {
      "nodeVersion": "18.x"
    },
    "cleanUrls": true,
    "public": "public"
  }